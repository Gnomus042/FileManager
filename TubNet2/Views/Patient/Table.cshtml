@model IEnumerable<DataLibrary.Patients>

<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.p_surname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.p_name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.p_secname)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.p_genderId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.p_birthday)
        </th>
        <th>

        </th>

    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.p_surname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.p_name)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.p_secname)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gender.gen_value)

            </td>
            <td>
                @Html.DisplayFor(modelItem => item.p_birthday)
            </td>
            <td>
                @Html.ActionLink("Переглянути", "SinglePatient", new { p_id = item.p_id }, htmlAttributes: new { @class = "btn btn-style btn-large" })

            </td>
        </tr>
    }

</table>