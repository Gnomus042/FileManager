@model IEnumerable<DataLibrary.Consult___Patient>

<div>
    <table class="table table-bordered table-striped">
        <tr>
            <th>@Html.DisplayNameFor(model => model.cp_date)</th>
            <th>@Html.DisplayNameFor(model => model.Consultation.cons_type)</th>
            <th>@Html.DisplayNameFor(model => model.Consultation.cons_komment)</th>
            <th></th>
        </tr>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.DisplayFor(modelItem => item.cp_date)</td>
            <td>@Html.DisplayFor(model => item.Consultation.ConsultationType.ct_value)</td>
            <td>@Html.DisplayFor(model => item.Consultation.cons_komment)</td>
    
            <td>
                @Html.ActionLink("Видалити", "DeleteTest", new { id = item.cp_id }, htmlAttributes: new { @class = "btn btn-style" })
            </td>
        </tr>
        }
    </table>
</div>